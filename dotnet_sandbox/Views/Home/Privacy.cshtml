@{
    ViewData["Title"] = "Configurações";
}

    <div class="text-center text-dark">
        <h1 class="display-4">@ViewData["Title"]</h1>
        <br />
    </div>

<div class="col-12 text-center">
    <table class="table table-hover table-dark">
        <thead>
            <tr class="text-white">
                <th scope="col">#</th>
                <th scope="col">Recurso</th>
                <th scope="col">Tipo</th>
                <th scope="col">Framework</th>
                <th scope="col">Versão</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">1</th>
                <td>Servidor</td>
                <td>Linux Ubuntu</td>
                <td>...</td>
                <td>18.04 LTS</td>
            </tr>
            <tr>
                <th scope="row">2</th>
                <td>Servidor - aplicação</td>
                <td>Kestrel</td>
                <td>...</td>
                <td>...</td>
            </tr>
            <tr>
                <th scope="row">3</th>
                <td>Servidor - proxy reverso</td>
                <td>Nginx</td>
                <td>...</td>
                <td>...</td>
            </tr>
            <tr>
                <th scope="row">4</th>
                <td>Aplicação</td>
                <td>Dotnet (.Net)</td>
                <td>ASP.NET Core MVC</td>
                <td>2.2</td>
            </tr>
            <tr>
                <th scope="row">5</th>
                <td>Banco de Dados</td>
                <td>...</td>
                <td>Entity Framework Core</td>
                <td>EF6.0</td>
            </tr>
            <tr>
                <th scope="row">6</th>
                <td>Banco de Dados</td>
                <td>SQL Server - Linux</td>
                <td>...</td>
                <td>MSSQLServer Express</td>
            </tr>
        </tbody>
    </table>
</div>

