@model Nomes

@{
    ViewData["Title"] = "Inicio";
}

<div class="text-center text-dark">
    <h1 class="display-4">SQL Server No linux</h1>
    <p>Insert.</p>
</div>

<div class="col-4 offset-4 text-center">
    <hr>
    <form asp-action="Create" asp-controller="Nomes">

        <div class="form-group">
            <input asp-for="Nome" class="form-control" placeholder="Seu nome" required>
        </div>

        <div class="form-group">
            <input asp-for="Snome" class="form-control" placeholder="Seu sobre nome" required>
        </div>

        <input type="submit" value="Registrar" class="btn btn-success" />
    </form>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
